---
import GitHub from "./icons/GitHub.astro";
const props = { ...Astro.props };
---

<a
  href=`${props.href}`
  target="_blank"
  class="my-4 inline-flex items-center justify-center py-2 px-3 text-base font-medium text-gray-500 rounded-lg bg-gray-50 hover:text-gray-900 hover:bg-gray-100 dark:text-gray-400 dark:bg-gray-800 dark:hover:bg-gray-700 dark:hover:text-white"
>
  <span class="w-full flex gap-x-3">
    <GitHub />Code
  </span>
</a>
